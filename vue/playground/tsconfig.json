{
	"compilerOptions": {
		"target": "ESNext",
		"lib": ["dom", "dom.iterable", "esnext"],
		"types": ["vite/client"],
		"allowJs": true,
		"skipLibCheck": true,
		"strict": false,
		"forceConsistentCasingInFileNames": true,
		"noEmit": true,
		"incremental": true,
		"esModuleInterop": true,
		"module": "esnext",
		"moduleResolution": "node",
		"downlevelIteration": true,
		"resolveJsonModule": true,
		"isolatedModules": true,
		"jsx": "preserve",
		"paths": {
			"@bitrix24/icons-vue/main/*": ["../main/esm/*"],
			"@bitrix24/icons-vue/actions/*": ["../actions/esm/*"],
			"@bitrix24/icons-vue/crm/*": ["../crm/esm/*"],
			"@bitrix24/icons-vue/contact-center/*": ["../contact-center/esm/*"],
			"@bitrix24/icons-vue/social/*": ["../social/esm/*"],
			"@bitrix24/icons-vue/common-b24/*": ["../common-b24/esm/*"],
			"@bitrix24/icons-vue/common-service/*": ["../common-service/esm/*"],
			"@bitrix24/icons-vue/editor/*": ["../editor/esm/*"],
			"@bitrix24/icons-vue/button/*": ["../button/esm/*"],
			"@bitrix24/icons-vue/button-specialized/*": ["../button-specialized/esm/*"],
		}
	},
	"include": ["**/*.ts", "**/*.tsx", "**/*.vue"],
	"exclude": ["node_modules"]
}