{
  "name": "@bitrix24/b24icons",
  "version": "2.0.5",
  "packageManager": "pnpm@10.20.0",
  "author": "Bitrix",
  "license": "MIT",
  "description": "Bitrix24 SVG icons for development web-applications",
  "keywords": [
    "bitrix24",
    "ui",
    "icons",
    "svg"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bitrix24/b24icons.git"
  },
  "homepage": "https://bitrix24.github.io/b24icons/",
  "bugs": {
    "url": "https://github.com/bitrix24/b24icons/issues"
  },
  "staring": {
    "url": "https://github.com/bitrix24/b24icons"
  },
  "type": "module",
  "private": true,
  "workspaces": [
    "packages/*",
    "docs"
  ],
  "scripts": {
    "vue": "pnpm --filter ./packages/@bitrix24-icons-vue build",
    "prepublishOnly": "pnpm run build",
    "prebuild": "rimraf --glob ./optimized/* ./export/* ./packages/{@bitrix24-icons-vue,@bitrix24-icons-react}/dist/* ./packages/@bitrix24-icons-vue/src/*.json",
    "build": "npm-run-all -s build:*",
    "build:by-type": "node ./scripts/build-process",
    "build:vue": "node ./scripts/build @bitrix24-icons-vue",
    "build:vue-packages": "pnpm run vue build",
    "build:nuxt-packages": "pnpm --filter ./packages/@bitrix24-icons-nuxt prepare",
    "build-nuxt": "npm-run-all -s build-nuxt:*",
    "build-nuxt:prepare": "pnpm --filter ./packages/@bitrix24-icons-nuxt prepare",
    "build-nuxt:build": "pnpm --filter ./packages/@bitrix24-icons-nuxt build",
    "docs:dev": "npm-run-all -s docs:dev:*",
    "docs:dev:init": "pnpm run build",
    "docs:dev:prepare": "nuxt prepare docs",
    "docs:dev:nuxt": "nuxt dev docs",
    "docs:build": "npm-run-all -s docs:build:*",
    "docs:build:init": "pnpm run build",
    "docs:build:nuxt": "nuxt build docs",
    "docs:generate": "npm-run-all -s docs:generate:*",
    "docs:generate:init": "pnpm run build",
    "docs:generate:nuxt": "nuxt generate docs",
    "docs:preview": "nuxt preview docs",
    "tools:catalog": "node ./scripts/catalog",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "vue-tsc --noEmit && nuxt typecheck docs"
  },
  "devDependencies": {
    "@nuxt/eslint-config": "^1.10.0",
    "@babel/core": "^7.28.5",
    "@babel/plugin-transform-react-jsx": "^7.27.1",
    "@svgr/core": "^8.1.0",
    "@vue/compiler-dom": "^3.5.22",
    "camelcase": "^8.0.0",
    "cross-env": "^10.1.0",
    "fast-glob": "^3.3.3",
    "mini-svg-data-uri": "^1.4.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.6.2",
    "rimraf": "^6.1.0",
    "svgo": "^4.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "vue-tsc": "^3.1.2",
    "eslint": "^9.38.0"
  },
  "dependencies": {
  },
  "resolutions": {
    "@bitrix24/b24ui-icons": "workspace:*"
  }
}
