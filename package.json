{
	"name": "@bitrix24/icons",
	"version": "0.1.0",
	"author": "Bitrix",
	"license": "MIT",
	"description": "Bitrix24 SVG icons for development web-applications",
	"keywords": [
		"bitrix24",
		"ui",
		"icons",
		"svg"
	],
	"homepage": "https://bitrix24.github.io/b24icons/",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/bitrix24/b24icons.git"
	},
	"files": [
		"actions/",
		"contact-center/",
		"crm/",
		"editor/",
		"main/",
		"social/",
		"button-specialized/",
		"button/",
		"common-service/",
		"common-b24/",
		"README.md",
		"LICENSE"
	],
	"workspaces": [
		"vue/playground"
	],
	"scripts": {
		"prebuild": "rimraf ./optimized/* ./export/* ./{vue,react}/{actions,contact-center,crm,editor,main,social,button-specialized,button,common-service,common-b24}",
		
		"build": "npm-run-all -s build:*",
		"build:by-type": "node ./scripts/build-process",
		"build:vue": "node ./scripts/build.js vue",
		
		"prepublishOnly": "npm run build",
		
		"playground-vue": "npm-run-all -s playground-vue:*",
		"playground-vue:build": "npm run build",
		"playground-vue:run-dev": "npm run --workspace=vue/playground dev",
		
		"docs:dev": "npm-run-all -s docs:dev:*",
		"docs:dev:init": "npm run build",
		"docs:dev:vitepress": "vitepress dev docs",
		
		"docs:build": "npm-run-all -s docs:build:*",
		"docs:build:init": "npm run build",
		"docs:build:vitepress": "vitepress build docs",
		
		"docs:preview": "vitepress preview docs"
	},
	"dependencies": {
	
	},
	"devDependencies": {
		"@babel/core": "^7.12.10",
		"@babel/plugin-transform-react-jsx": "^7.12.12",
		"@svgr/core": "^5.5.0",
		"@vue/compiler-dom": "^3.0.5",
		"camelcase": "^6.0.0",
		"prettier": "^2.8.7",
		"npm-run-all": "^4.1.5",
		"ts-node": "^10.0.0",
		"typescript": "^5.4.3",
		"rimraf": "^3.0.2",
		"svgo": "^3.0.2",
		"vitepress": "^1.3.4",
		"vue": "^3.4.27",
		"vue-router": "^4.3.2",
        "@tailwindcss/forms": "^0.5.7",
        "@tailwindcss/typography": "^0.5.2",
        "tailwindcss": "^3.4.10",
        "autoprefixer": "^10.4.7",
        "postcss": "^8.4.14"
    }
}