{
  "name": "@bitrix24/b24icons-nuxt",
  "version": "2.0.5",
  "author": "Bitrix",
  "license": "MIT",
  "description": "Bitrix24 SVG icons for development web-applications for Nuxt",
  "keywords": [
    "bitrix24",
    "ui",
    "icons",
    "svg",
    "nuxt"
  ],
  "homepage": "https://bitrix24.github.io/b24icons/docs/getting-started/installation/nuxt/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bitrix24/b24icons.git",
    "directory": "packages/@bitrix24-icons-nuxt"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs"
    }
  },
  "main": "./dist/module.mjs",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/module.d.mts"
      ]
    }
  },
  "files": [
    "README.md",
    "LICENSE",
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prepare": "nuxt-module-build build --stub && nuxt-module-build prepare",
    "prepack": "pnpm build",
    "build": "nuxt-module-build build"
  },
  "dependencies": {
    "@nuxt/kit": "^4.2.1",
    "@bitrix24/b24icons-vue": "workspace:*"
  },
  "devDependencies": {
    "@nuxt/devtools": "^3.1.0",
    "@nuxt/eslint-config": "^1.10.0",
    "@nuxt/module-builder": "^1.0.2",
    "@nuxt/schema": "^4.2.1",
    "@types/node": "^24.10.1",
    "eslint": "^9.39.1",
    "nuxt": "^4.2.1",
    "typescript": "^5.9.3",
    "vue-tsc": "^3.1.3"
  }
}
